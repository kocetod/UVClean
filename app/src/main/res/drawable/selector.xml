<?xml version = "1.0" encoding = "utf-8"?>
<selector xmlns:android = "http://schemas.android.com/apk/res/android">
    <item android:state_checked="false" android:drawable="@drawable/ic_power_on">
        <shape android:shape = "oval">
            <solid android:color = "#CDB8B8B8"/>
        </shape>
    </item>
    <item android:state_checked = "true" android:drawable="@drawable/ic_power_off">
        <shape android:shape = "oval">
            <solid android:color = "#CDB8B8B8"/>
        </shape>
    </item>
</selector>